window.onload = function() {
		loadpopjs();
		//loadpopjs1();
		//loadpopjs2();

    };
	
	


	

function loadpopjs() {
        var script = document.createElement('script');
        
        // 设置该<script>元素的src属性，指向外部JS文件
        script.src = './Optimize_Android_interface_display_language_script.js';

        // 将<script>元素插入到<body>中
        document.body.appendChild(script);
		
		
}

function loadpopjs1() {
            var link = document.createElement('link');
            link.rel = 'stylesheet';
            link.type = 'text/css';
            link.href = './CSS/pop.css'; // 替换为你的CSS文件路径
            document.head.appendChild(link);
}

function loadpopjs2() {
            var link = document.createElement('link');
            link.rel = 'stylesheet';
            link.type = 'text/css';
            link.href = './CSS/localization_Win.css'; // 替换为你的CSS文件路径
            document.head.appendChild(link);
}

	